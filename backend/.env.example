# Server Configuration
PORT=4000
NODE_ENV=development

# Alchemy Configuration
ALCHEMY_API_KEY=your_alchemy_api_key_here
ALCHEMY_NETWORK=polygon-amoy

# ChainEquity Token Contract
TOKEN_CONTRACT_ADDRESS=your_deployed_contract_address_here

# Issuer Private Key (for signing transactions)
# This should be the private key of the wallet that will execute transactions
# Can fallback to DEPLOYER_PRIVATE_KEY if not set
ISSUER_PRIVATE_KEY=your_private_key_here

# Database
DATABASE_PATH=./data/chainequity.db
